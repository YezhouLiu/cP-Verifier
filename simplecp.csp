//simplecp
var a;
var b;
var d;
var c;
var state = 1;

S0() = cp_init{
a = 10;
b = 0;
d = 0;
c = 0;
}-> S1();
S1() = r1{
if (state == 1 && a > 0){
a = a - 2;
b = b + 1;
state = 1;
}
}-> S1();
S2() = r2{
if (state == 1 && b > 0){
b = b - 2;
c = c + 1;
d = d + 2;
state = 1;
}
}-> S1();
#assert S0() |= F r1;
#assert S0() |= G r2;

